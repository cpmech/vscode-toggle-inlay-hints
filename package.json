{
  "name": "vscode-toggle-inlay-hints",
  "displayName": "Toggle Inlay Hints",
  "description": "A status bar item to toggle inlay hints",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.67.0"
  },
  "categories": [
    "Other"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/cpmech/vscode-toggle-inlay-hints.git"
  },
  "publisher": "cpmech",
  "author": "Dorival Pedroso (dorival.pedroso@gmail.com)",
  "icon": "icon.png",
  "bugs": {
    "url": "https://github.com/cpmech/toggle-inlay-hints/issues"
  },
  "homepage": "https://marketplace.visualstudio.com/items?itemName=cpmech.vscode-toggle-inlay-hints",
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vscode-toggle-inlay-hints.doToggle",
        "title": "Toggle Inlay Hints"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "pkg": "vsce package",
    "add2code": "code --install-extension vscode-toggle-inlay-hints-0.0.1.vsix"
  },
  "devDependencies": {
    "@types/vscode": "^1.67.0",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "14.x",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "eslint": "^8.14.0",
    "glob": "^8.0.1",
    "mocha": "^9.2.2",
    "typescript": "^4.6.4",
    "@vscode/test-electron": "^2.1.3"
  }
}
